# AlgoMaster-Studio AI Engine Environment Variables

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_RELOAD=true

# AI Service Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
AI_MODEL_PRIMARY=gpt-4
AI_MODEL_SECONDARY=gpt-3.5-turbo
AI_MAX_TOKENS=4000
AI_TEMPERATURE=0.3

# Security Configuration
JWT_SECRET_KEY=algomaster-studio-super-secret-key-change-in-production-2024
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30
SECURITY_ENABLED=true

# Database Configuration (for future use)
DATABASE_URL=postgresql://user:password@localhost:5432/algomaster_db
REDIS_URL=redis://localhost:6379/0

# Performance Configuration
MAX_EXECUTION_TIME=30
MAX_MEMORY_USAGE=512
MAX_CONCURRENT_ANALYSES=10
BENCHMARK_TIMEOUT=60

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_ROTATION=daily
LOG_RETENTION_DAYS=30

# Docker Configuration
DOCKER_ENABLED=true
DOCKER_TIMEOUT=30
DOCKER_MEMORY_LIMIT=256m
DOCKER_CPU_LIMIT=1

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_REQUESTS_PER_HOUR=1000

# Development/Production Settings
ENVIRONMENT=development
DEBUG=true
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
ALLOWED_HOSTS=localhost,127.0.0.1

# Analytics and Monitoring
ANALYTICS_ENABLED=true
METRICS_ENABLED=true
HEALTH_CHECK_INTERVAL=30

# File Upload Configuration
MAX_FILE_SIZE=10MB
ALLOWED_EXTENSIONS=.py,.cpp,.java,.js,.ts
UPLOAD_DIR=./uploads

# Visualization Configuration
VIZ_CACHE_ENABLED=true
VIZ_CACHE_TTL=3600
VIZ_MAX_NODES=1000

# AI Model Configuration
AI_CACHE_ENABLED=true
AI_CACHE_TTL=1800
AI_RETRY_ATTEMPTS=3
AI_RETRY_DELAY=1

# Performance Monitoring
PERFORMANCE_MONITORING=true
SLOW_QUERY_THRESHOLD=5
MEMORY_USAGE_THRESHOLD=80
CPU_USAGE_THRESHOLD=80