{
  "name": "algomaster-studio-frontend",
  "version": "2.0.0",
  "description": "Revolutionary AI-powered algorithm learning platform - Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@tanstack/react-query": "^5.8.4",
    "@monaco-editor/react": "^4.6.0",
    "monaco-editor": "^0.44.0",
    "d3": "^7.8.5",
    "@types/d3": "^7.4.3",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.294.0",
    "zustand": "^4.4.7",
    "axios": "^1.6.2",
    "react-hot-toast": "^2.4.1",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "react-syntax-highlighter": "^15.5.0",
    "@types/react-syntax-highlighter": "^15.5.11",
    "recharts": "^2.8.0",
    "react-split-pane": "^0.1.92",
    "@types/react-split-pane": "^0.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0",
    "typescript": "^5.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "vitest": "^0.34.6",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "jsdom": "^23.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "algorithms",
    "data-structures", 
    "ai-powered",
    "learning-platform",
    "code-analysis",
    "visualization",
    "react",
    "typescript",
    "educational"
  ],
  "author": "AlgoMaster Studio Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/VenkataAnilKumar/AlgoMaster-Studio.git"
  },
  "bugs": {
    "url": "https://github.com/VenkataAnilKumar/AlgoMaster-Studio/issues"
  },
  "homepage": "https://algomaster.studio"
}